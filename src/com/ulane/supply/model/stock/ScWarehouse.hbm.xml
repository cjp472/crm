<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.supply.model.stock.ScWarehouse"
        table="SC_WAREHOUSE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="warehouseId"
            column="WAREHOUSE_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_SC_WAREHOUSE_ID
            	</param>
            </generator>
        </id>
         <property
            name="warehouseName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="WAREHOUSE_NAME"
            length="60"
            not-null="true"
            unique="false"
        />
        <property
            name="warehouseNo"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="WAREHOUSE_NO"
            length="60"
            not-null="true"
            unique="false"
        />
        <property
            name="warehousePinyin"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="WAREHOUSE_PINYIN"
            length="60"
            not-null="true"
            unique="false"
        />
        <property
            name="warehouseMgr"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="WAREHOUSE_MGR"
            length="18"
            not-null="true"
            unique="false"
        />
        <property
            name="whCellphone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="WH_CELLPHONE"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="whPhone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="WH_PHONE"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="whFax"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="WH_FAX"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="whAddr"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="WH_ADDR"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="whPostcode"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="WH_POSTCODE"
            length="6"
            not-null="false"
            unique="false"
        />
        <property
            name="ownerDeptId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="OWNER_DEPT_ID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="coverArea"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="COVER_AREA"
            length="60"
            not-null="false"
            unique="false"
        />
        <property
            name="warehouseDesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="WAREHOUSE_DESC"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="warehouseStatus"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="WAREHOUSE_STATUS"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="createUserId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CREATE_USER_ID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_TIME"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updateUserId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_USER_ID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="updateTime2"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_TIME2"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="desc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DESC"
            length="500"
            not-null="false"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to ScBizSalesDetail -->
	<set name="scBizSalesDetails"
	     table="sc_biz_sales_detail"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="WAREHOUSE_ID"/>
		</key>
		<one-to-many
			class="com.ulane.supply.model.sales.ScBizSalesDetail"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ScBoPurchaseDetail -->
	<set name="scBoPurchaseDetails"
	     table="sc_bo_purchase_detail"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="WAREHOUSE_ID"/>
		</key>
		<one-to-many
			class="com.ulane.supply.model.purchase.ScBoPurchaseDetail"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ScBoStockDetail -->
	<set name="scBoStockDetails"
	     table="sc_bo_stock_detail"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="WAREHOUSE_ID"/>
		</key>
		<one-to-many
			class="com.ulane.supply.model.stock.ScBoStockDetail"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ScGoodsStock -->
	<set name="scGoodsStocks"
	     table="sc_goods_stock"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="WAREHOUSE_ID"/>
		</key>
		<one-to-many
			class="com.ulane.supply.model.stock.ScGoodsStock"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ScThresholdLevel -->
	<set name="scThresholdLevels"
	     table="sc_threshold_level"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="WAREHOUSE_ID"/>
		</key>
		<one-to-many
			class="com.ulane.supply.model.stock.ScThresholdLevel"
		/>
	</set>     
    </class>
</hibernate-mapping>
