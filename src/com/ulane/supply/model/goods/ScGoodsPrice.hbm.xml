<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.supply.model.goods.ScGoodsPrice"
        table="SC_GOODS_PRICE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="goodsPriceId"
            column="GOODS_PRICE_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_SC_GOODS_PRICE_ID
            	</param>
            </generator>
        </id>
         <property
            name="purchasePrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="PURCHASE_PRICE"
            precision="15"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="reportPrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="REPORT_PRICE"
            precision="15"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="retailPrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="RETAIL_PRICE"
            precision="15"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="wholesalePrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="WHOLESALE_PRICE"
            precision="15"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="defaultSubsidyAmount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="DEFAULT_SUBSIDY_AMOUNT"
            precision="15"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="createUserId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CREATE_USER_ID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_TIME"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updateUserId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_USER_ID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="updateTime2"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_TIME2"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="desc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="DESC_"
            length="500"
            not-null="false"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to ScGoods -->
	<set name="scGoodss"
	     table="sc_goods"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="GOODS_PRICE_ID"/>
		</key>
		<one-to-many
			class="com.ulane.supply.model.goods.ScGoods"
		/>
	</set>     
    </class>
</hibernate-mapping>
