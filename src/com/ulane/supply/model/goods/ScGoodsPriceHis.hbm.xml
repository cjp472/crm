<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.supply.model.goods.ScGoodsPriceHis"
        table="SC_GOODS_PRICE_HIS"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="goodsPriceHisSeq"
            column="GOODS_PRICE_HIS_SEQ"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_SC_GOODS_PRICE_HIS_ID
            	</param>
            </generator>
        </id>
         <property
            name="purchasePrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="PURCHASE_PRICE"
            precision="15"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="reportPrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="REPORT_PRICE"
            precision="15"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="retailPrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="RETAIL_PRICE"
            precision="15"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="wholesalePrice"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="WHOLESALE_PRICE"
            precision="15"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="defaultSubsidyAmount"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="DEFAULT_SUBSIDY_AMOUNT"
            precision="15"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="updateTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_TIME"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="optUserId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="OPT_USER_ID"
            length="18"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to ScGoods -->
		<many-to-one
			name="scGoods"
			class="com.ulane.supply.model.goods.ScGoods"
            not-null="false"
			fetch="select"
		>	
			<column name="GOODS_ID" />
		</many-to-one>
    </class>
</hibernate-mapping>
