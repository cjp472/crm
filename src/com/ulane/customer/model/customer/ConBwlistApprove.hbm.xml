<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.customer.model.customer.ConBwlistApprove"
        table="CON_BWLIST_APPROVE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="bwlistApproveId"
            column="BWLIST_APPROVE_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_CON_BWLIST_APPROVE_ID
            	</param>
            </generator>
        </id>
         <property
            name="approveTitle"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPROVE_TITLE"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="approveComment"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPROVE_COMMENT"
            length="300"
            not-null="false"
            unique="false"
        />
        <property
            name="bwlistStatus"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="BWLIST_STATUS"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="runid"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="RUNID"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
<!--    <property
            name="userid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="USERID"
            length="18"
            not-null="false"
            unique="false"
        /> -->
        <property
            name="isDelete"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="IS_DELETE"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="nodeName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="NODE_NAME"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="approvalStatus"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPROVAL_STATUS"
            length="30"
            not-null="false"
            unique="false"
        />
		<many-to-one name="applyUser" class="com.htsoft.oa.model.system.AppUser" not-null="false" fetch="select"> 
	      <column name="USERID"/> 
	    </many-to-one>
  <!-- exported bi-directional one-to-many association to ConBwList -->
	<set name="conBwLists"
	     table="con_bw_list"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="BWLIST_APPROVE_ID"/>
		</key>
		<one-to-many
			class="com.ulane.customer.model.customer.ConBwList"
		/>
	</set>     
    </class>
</hibernate-mapping>
