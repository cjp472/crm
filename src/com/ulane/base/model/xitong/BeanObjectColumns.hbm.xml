<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.base.model.xitong.BeanObjectColumns"
        table="BEAN_OBJECT_COLUMNS"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="beanObjectColumnsId"
            column="BEAN_OBJECT_COLUMNS_ID"
            type="java.lang.Long"
            length="38"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_BEAN_OBJECT_COLUMNS_ID
            	</param>
            </generator>
        </id>
         <property
            name="beanObjectColumns"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="BEAN_OBJECT_COLUMNS"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="beanObjectColumnsName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="BEAN_OBJECT_COLUMNS_NAME"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="beanObjectColumnsTame"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="BEAN_OBJECT_COLUMNS_TAME"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="38"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="38"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="comment"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="COMMENTS"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="isExt"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="IS_EXT"
            length="2"
            not-null="false"
            unique="false"
        />
        <property
            name="columnType"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="TYPE"
            length="5"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to BeanObject -->
		<many-to-one
			name="beanObject"
			class="com.ulane.base.model.xitong.BeanObject"
            not-null="false"
			fetch="select"
		>	
			<column name="BEAN_OBJECT_ID" />
		</many-to-one>
    </class>
</hibernate-mapping>
