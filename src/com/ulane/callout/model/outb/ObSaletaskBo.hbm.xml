<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.callout.model.outb.ObSaletaskBo"
        table="OB_SALETASK_BO"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="saletaskBoId"
            column="SALETASK_BO_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_OB_SALETASK_BO_ID
            	</param>
            </generator>
        </id>
         <property
            name="booTim"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="BOO_TIM"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="conCusId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CON_CUS_ID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="tasMetId"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="TAS_MET_ID"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="conNumber"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CON_NUMBER"
            length="256"
            not-null="false"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REMARK"
            length="1024"
            not-null="false"
            unique="false"
        />
        <property
            name="booStaId"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="BOO_STA_ID"
            length="5"
            not-null="true"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to ObSaletask -->
		<many-to-one
			name="obSaletask"
			class="com.ulane.callout.model.outb.ObSaletask"
            not-null="false"
			fetch="select"
		>	
			<column name="SALETASK_ID" />
		</many-to-one>
    </class>
</hibernate-mapping>
