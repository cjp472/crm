<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.monitor.model.unim.UnimAgentloginlog"
        table="UNIM_AGENTLOGINLOG"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="logId"
            column="LOG_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			S_UNIM_AGENTLOGINLOG
            	</param>
            </generator>
        </id>
         <property
            name="aid"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="AID"
            length="15"
            not-null="true"
            unique="false"
        />
         <property
            name="station"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="STATION"
            length="15"
            not-null="true"
            unique="false"
        />
        <property
            name="locId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="LOC_ID"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="alTenant"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="AL_TENANT"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="loginDate"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="LOGIN_DATE"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="loginTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="LOGIN_TIME"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="logoutDate"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="LOGOUT_DATE"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="logoutTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="LOGOUT_TIME"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="logoutReason"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="LOGOUT_REASON"
            length="5"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
