<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.running.model.qucon.QcTempRelease"
        table="QC_TEMP_RELEASE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="tempReleId"
            column="TEMP_RELE_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_QC_TEMP_RELEASE_ID
            	</param>
            </generator>
        </id>
         <property
            name="chkChannel"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="CHK_CHANNEL"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="releName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="RELE_NAME"
            length="256"
            not-null="true"
            unique="false"
        />
        <property
            name="releContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="RELE_CONTENT"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="chkTypeId"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="CHK_TYPE_ID"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="allowRemark"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="ALLOW_REMARK"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="allowRecheck"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="ALLOW_RECHECK"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="baseScore"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="BASE_SCORE"
            precision="5"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="minScore"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="MIN_SCORE"
            precision="5"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="maxScore"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="MAX_SCORE"
            precision="5"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="staDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="STA_DAT"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="endDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="END_DAT"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="releaseFilePath"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="RELEASE_FILE_PATH"
            length="2048"
            not-null="true"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REMARK"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="relaseUseId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="RELASE_USE_ID"
            length="18"
            not-null="true"
            unique="false"
        />
        <property
            name="relaseDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="RELASE_DAT"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="staId"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STA_ID"
            length="5"
            not-null="true"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to QcTemplate -->
		<many-to-one
			name="qcTemplate"
			class="com.ulane.running.model.qucon.QcTemplate"
            not-null="false"
			fetch="select"
		>	
			<column name="TMP_ID" />
		</many-to-one>
 <!-- exported bi-directional one-to-many association to QcCheck -->
	<set name="qcChecks"
	     table="qc_check"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="TEMP_RELE_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.qucon.QcCheck"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to QcTempReObj -->
	<set name="qcTempReObjs"
	     table="qc_temp_re_obj"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="TEMP_RELE_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.qucon.QcTempReObj"
		/>
	</set>
	<set name="qcTempReChas"
	     table="qc_temp_re_cha"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted">
		<key>
			<column name="TEMP_RELE_ID"/>
		</key>
		<one-to-many class="com.ulane.running.model.qucon.QcTempReCha"/>
	</set>         
    </class>
</hibernate-mapping>
