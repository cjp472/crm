<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.running.model.qucon.QcTemplate"
        table="QC_TEMPLATE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="tmpId"
            column="TMP_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_QC_TEMPLATE_ID
            	</param>
            </generator>
        </id>
         <property
            name="tmpName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="TMP_NAME"
            length="256"
            not-null="true"
            unique="false"
        />
        <property
            name="tmpContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="TMP_CONTENT"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="chkTypeId"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="CHK_TYPE_ID"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="allowRemark"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="ALLOW_REMARK"
            length="5"
            not-null="true"
            unique="false"
        />
         <property
            name="allowTarRemark"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="IS_TAR_REMARK"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="allowRecheck"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="ALLOW_RECHECK"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="baseScore"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="BASE_SCORE"
            precision="5"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="minScore"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="MIN_SCORE"
            precision="5"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="maxScore"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="MAX_SCORE"
            precision="5"
            scale="2"
            not-null="true"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REMARK"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="creUseId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CRE_USE_ID"
            length="18"
            not-null="true"
            unique="false"
        />
        <property
            name="creDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CRE_DAT"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="updUseId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="UPD_USE_ID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="updDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPD_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="staId"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STA_ID"
            length="5"
            not-null="true"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to QcScoreOpt -->
	<set name="qcScoreOpts"
	     table="qc_score_opt"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     order-by="DISORDER asc">
		<key>
			<column name="TMP_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.qucon.QcScoreOpt"/>
	</set>     
 <!-- exported bi-directional one-to-many association to QcTempChapcter -->
	<set name="qcTempChapcters"
	     table="qc_temp_chapcter"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     order-by="DISORDER asc">
		<key>
			<column name="TMP_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.qucon.QcTempChapcter"/>
	</set>     
 <!-- exported bi-directional one-to-many association to QcTempRelease -->
	<set name="qcTempReleases"
	     table="qc_temp_release"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="TMP_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.qucon.QcTempRelease"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to QcTempTar -->
	<set name="qcTempTars"
	     table="qc_temp_tar"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted">
		<key>
			<column name="TMP_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.qucon.QcTempTar"/>
	</set>     
    </class>
</hibernate-mapping>
