<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.running.model.comtech.CtScrRelease"
        table="CT_SCR_RELEASE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="scrId"
            column="SCR_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_CT_SCR_RELEASE_ID
            	</param>
            </generator>
        </id>
         <property
            name="scrName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SCR_NAME"
            length="256"
            not-null="true"
            unique="false"
        />
        <property
            name="scrContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="SCR_CONTENT"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="staDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="STA_DAT"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="endDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="END_DAT"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="busiDir"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="BUSI_DIR"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="userUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="USER_USER"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="userUsegroup"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="USER_USEGROUP"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="userSkill"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="USER_SKILL"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="releaseFilePath"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="RELEASE_FILE_PATH"
            length="2048"
            not-null="true"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REMARK"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="relaseUseId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="RELASE_USE_ID"
            length="18"
            not-null="true"
            unique="false"
        />
        <property
            name="relaseDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="RELASE_DAT"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="staId"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STA_ID"
            length="5"
            not-null="true"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to CtScrTemplate -->
		<many-to-one
			name="ctScrTemplate"
			class="com.ulane.running.model.comtech.CtScrTemplate"
            not-null="false"
			fetch="select"
		>	
			<column name="TMP_ID" />
		</many-to-one>
 <!-- exported bi-directional one-to-many association to CtScrAnsSummary -->
	<set name="ctScrAnsSummarys"
	     table="ct_scr_ans_summary"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="SCR_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.comtech.CtScrAnsSummary"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to CtScrReleaseObj -->
	<set name="ctScrReleaseObjs"
	     table="ct_scr_release_obj"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="SCR_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.comtech.CtScrReleaseObj"
		/>
	</set>     
    </class>
</hibernate-mapping>
