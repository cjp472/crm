<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.running.model.pap.PapTemplate"
        table="PAP_TEMPLATE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="tmpId"
            column="TMP_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_PAP_TEMPLATE_ID
            	</param>
            </generator>
        </id>
         <property
            name="tmpName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="TMP_NAME"
            length="256"
            not-null="true"
            unique="false"
        />
        <property
            name="tmpContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="TMP_CONTENT"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="displayLayoutId"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="DISPLAY_LAYOUT_ID"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="displayStyleId"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="DISPLAY_STYLE_ID"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="REMARK"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="guideFilePath"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="GUIDE_FILE_PATH"
            length="2048"
            not-null="false"
            unique="false"
        />
        <property
            name="applyAnsNo"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="APPLY_ANS_NO"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="needPassChk"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="NEED_PASS_CHK"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="applyAnsMuti"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="APPLY_ANS_MUTI"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="applyViewRes"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="APPLY_VIEW_RES"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="displayGuide"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="DISPLAY_GUIDE"
            length="5"
            not-null="true"
            unique="false"
        />
        <property
            name="creUseId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CRE_USE_ID"
            length="18"
            not-null="true"
            unique="false"
        />
        <property
            name="creDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CRE_DAT"
            length="7"
            not-null="true"
            unique="false"
        />
        <property
            name="updUseId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="UPD_USE_ID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="updDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPD_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="staId"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STA_ID"
            length="5"
            not-null="true"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to PapChapcter -->
	<set name="papChapcters"
	     table="pap_chapcter"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="TMP_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.pap.PapChapcter"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to PapRelease -->
	<set name="papReleases"
	     table="pap_release"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="TMP_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.pap.PapRelease"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to PapTemGotoRule -->
	<set name="papTemGotoRules"
	     table="pap_tem_goto_rule"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="TMP_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.pap.PapTemGotoRule"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to PapTemQue -->
	<set name="papTemQues"
	     table="pap_tem_que"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="TMP_ID"/>
		</key>
		<one-to-many
			class="com.ulane.running.model.pap.PapTemQue"
		/>
	</set>     
    </class>
</hibernate-mapping>
