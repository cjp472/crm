  <!-- exported bi-directional one-to-many association to ShBugCus -->
	<set name="shBugCuss"
	     table="sh_bug_cus"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="APPLY_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShBugCus"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShBugRepaire -->
