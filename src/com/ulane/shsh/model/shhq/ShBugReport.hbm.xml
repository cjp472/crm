		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShBugRepaire"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShReportFile -->
	<set name="shReportFiles"
	     table="sh_report_file"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="APPLY_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShReportFile"
		/>
	</set>     
    </class>
