<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.shsh.model.shhq.ShBuilderControl"
        table="SH_BUILDER_CONTROL"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="contlId"
            column="CONTL_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_SH_BUILDER_CONTROL_ID
            	</param>
            </generator>
        </id>
         <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="TITLE"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CODE"
            length="60"
            not-null="false"
            unique="false"
        />
        <property
            name="contlContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CONTL_CONTENT"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="applyAddress"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPLY_ADDRESS"
            length="300"
            not-null="false"
            unique="false"
        />
        <property
            name="hourseName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="HOURSE_NAME"
            length="300"
            not-null="false"
            unique="false"
        />
        <property
            name="resource"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="RESOURCE"
            length="300"
            not-null="false"
            unique="false"
        />
        <property
            name="applyUserid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="APPLY_USERID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="applyDepid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="APPLY_DEPID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="perIncharge"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="PER_INCHARGE"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="applyDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="APPLY_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STATUS"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="signPerson"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="SIGN_PERSON"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="creUserid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CRE_USERID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="creDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CRE_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to ShBuilderApply -->
		<many-to-one
			name="shBuilderApply"
			class="com.ulane.shsh.model.shhq.ShBuilderApply"
            not-null="false"
			fetch="select"
		>	
			<column name="APPLY_ID" />
		</many-to-one>
 <!-- exported bi-directional one-to-many association to ShAcptCtrl -->
	<set name="shAcptCtrls"
	     table="sh_acpt_ctrl"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="CONTL_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShAcptCtrl"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShCtrlFile -->
	<set name="shCtrlFiles"
	     table="sh_ctrl_file"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="CONTL_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShCtrlFile"
		/>
	</set>     
    </class>
</hibernate-mapping>
