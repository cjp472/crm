<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.shsh.model.shhq.ShBuilderMethod"
        table="SH_BUILDER_METHOD"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="metdId"
            column="METD_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_SH_BUILDER_METHOD_ID
            	</param>
            </generator>
        </id>
         <property
            name="buildType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="BUILD_TYPE"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="applyContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPLY_CONTENT"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="staDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="STA_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="endDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="END_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="buildPerson"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="BUILD_PERSON"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="buildDepid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="BUILD_DEPID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="perIncharge"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="PER_INCHARGE"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STATUS"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="runid"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="RUNID"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="userid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="USERID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="applyDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="APPLY_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="TITLE"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="isDelete"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="IS_DELETE"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="nodeName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="NODE_NAME"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="approvalStatus"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPROVAL_STATUS"
            length="30"
            not-null="false"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to ShAcptMetd -->
	<set name="shAcptMetds"
	     table="sh_acpt_metd"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="METD_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShAcptMetd"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShBuilderApply -->
	<set name="shBuilderApplys"
	     table="sh_builder_apply"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="METD_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShBuilderApply"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShMetdFile -->
	<set name="shMetdFiles"
	     table="sh_metd_file"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="METD_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShMetdFile"
		/>
	</set>     
    </class>
</hibernate-mapping>
