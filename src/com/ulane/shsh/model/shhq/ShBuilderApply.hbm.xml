<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.shsh.model.shhq.ShBuilderApply"
        table="SH_BUILDER_APPLY"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="applyId"
            column="APPLY_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_SH_BUILDER_APPLY_ID
            	</param>
            </generator>
        </id>
         <property
            name="applyUserid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="APPLY_USERID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="applyTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="APPLY_TIME"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="applyTitle"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPLY_TITLE"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="applyComment"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPLY_COMMENT"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="applyContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPLY_CONTENT"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="applyDescribe"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPLY_DESCRIBE"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CODE"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="buldAddress"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="BULD_ADDRESS"
            length="300"
            not-null="false"
            unique="false"
        />
        <property
            name="buildHouse"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="BUILD_HOUSE"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="buildArear"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="BUILD_AREAR"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="isFire"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="IS_FIRE"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="isHeight"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="IS_HEIGHT"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="perIncharge"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="PER_INCHARGE"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="perCall"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="PER_CALL"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="perPhone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="PER_PHONE"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="applyStatus"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="APPLY_STATUS"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="runid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="RUNID"
            length="38"
            not-null="false"
            unique="false"
        />
        <property
            name="createBy"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CREATE_BY"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="updateBy"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_BY"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CREATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updateDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPDATE_DATE"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="nodeName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="NODE_NAME"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="approvalStatus"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPROVAL_STATUS"
            length="30"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to ShBuilderMethod -->
		<many-to-one
			name="shBuilderMethod"
			class="com.ulane.shsh.model.shhq.ShBuilderMethod"
            not-null="false"
			fetch="select"
		>	
			<column name="METD_ID" />
		</many-to-one>
 <!-- exported bi-directional one-to-many association to ShAcptApply -->
	<set name="shAcptApplys"
	     table="sh_acpt_apply"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="APPLY_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShAcptApply"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShBuilderControl -->
	<set name="shBuilderControls"
	     table="sh_builder_control"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="APPLY_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShBuilderControl"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShBuilderTime -->
	<set name="shBuilderTimes"
	     table="sh_builder_time"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="APPLY_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShBuilderTime"
		/>
	</set>     
    </class>
</hibernate-mapping>
