<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.shsh.model.shhq.ShBugRepaire"
        table="SH_BUG_REPAIRE"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="repId"
            column="REP_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_SH_BUG_REPAIRE_ID
            	</param>
            </generator>
        </id>
         <property
            name="applyContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPLY_CONTENT"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STATUS"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="applyPersons"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPLY_PERSONS"
            length="300"
            not-null="false"
            unique="false"
        />
        <property
            name="staDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="STA_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="finishDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="FINISH_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="applyResult"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="APPLY_RESULT"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="unApplyReason"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="UN_APPLY_REASON"
            length="1024"
            not-null="false"
            unique="false"
        />
        <property
            name="creUserid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CRE_USERID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="perIncharge"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="PER_INCHARGE"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="creDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CRE_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPD_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
     <!-- imported bi-directional many-to-one association to ShBugReport -->
		<many-to-one
			name="shBugReport"
			class="com.ulane.shsh.model.shhq.ShBugReport"
            not-null="false"
			fetch="select"
		>	
			<column name="APPLY_ID" />
		</many-to-one>
 <!-- exported bi-directional one-to-many association to ShBugRepStuff -->
	<set name="shBugRepStuffs"
	     table="sh_bug_rep_stuff"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="REP_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShBugRepStuff"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShRepaireFile -->
	<set name="shRepaireFiles"
	     table="sh_repaire_file"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="REP_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShRepaireFile"
		/>
	</set>     
    </class>
</hibernate-mapping>
