<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.ulane.shsh.model.shhq.ShBuilderAccept"
        table="SH_BUILDER_ACCEPT"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="acptId"
            column="ACPT_ID"
            type="java.lang.Long"
            length="18"
        >
             <generator class="sequence">
            	<param name="sequence">
			SEQ_SH_BUILDER_ACCEPT_ID
            	</param>
            </generator>
        </id>
         <property
            name="title"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="TITLE"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="CODE"
            length="128"
            not-null="false"
            unique="false"
        />
        <property
            name="applyUserid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="APPLY_USERID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="applyDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="APPLY_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="applyContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPLY_CONTENT"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="perIncharge"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="PER_INCHARGE"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="perCall"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="PER_CALL"
            length="60"
            not-null="false"
            unique="false"
        />
        <property
            name="perPhone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="PER_PHONE"
            length="60"
            not-null="false"
            unique="false"
        />
        <property
            name="acptResult"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="ACPT_RESULT"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="acptContent"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="ACPT_CONTENT"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="creUserid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="CRE_USERID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="creDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="CRE_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="updUserid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="UPD_USERID"
            length="18"
            not-null="false"
            unique="false"
        />
        <property
            name="updDat"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="UPD_DAT"
            length="7"
            not-null="false"
            unique="false"
        />
        <property
            name="nodeName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="NODE_NAME"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="approvalStatus"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="APPROVAL_STATUS"
            length="30"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="STATUS"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="runid"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="RUNID"
            length="10"
            not-null="false"
            unique="false"
        />
        <property
            name="isDelete"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="IS_DELETE"
            length="10"
            not-null="false"
            unique="false"
        />
  <!-- exported bi-directional one-to-many association to ShAcptApply -->
	<set name="shAcptApplys"
	     table="sh_acpt_apply"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="ACPT_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShAcptApply"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShAcptCtrl -->
	<set name="shAcptCtrls"
	     table="sh_acpt_ctrl"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="ACPT_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShAcptCtrl"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShAcptFile -->
	<set name="shAcptFiles"
	     table="sh_acpt_file"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="ACPT_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShAcptFile"
		/>
	</set>     
 <!-- exported bi-directional one-to-many association to ShAcptMetd -->
	<set name="shAcptMetds"
	     table="sh_acpt_metd"
	     lazy="true"
	     inverse="true"
	     cascade="save-update"
	     sort="unsorted"
	>
		<key>
			<column name="ACPT_ID"/>
		</key>
		<one-to-many
			class="com.ulane.shsh.model.shhq.ShAcptMetd"
		/>
	</set>     
    </class>
</hibernate-mapping>
